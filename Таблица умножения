using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace SoloLearn
{
	class Program
	{
		static void Main(string[] args)
		{
        for (int i=2;i<=6;i++)
        {
        for (int j=2;j<=6;j++)
        {
		Console.WriteLine("{0}*{1}={2}",i,j,i*j);
        }
        Console.WriteLine();
        }
		}
	}
}
