using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace SoloLearn
{
	class Program
	{
		static void Main(string[] args)
		{
            int x=1;
            int s=Convert.ToInt32(Console.ReadLine());
            for (int i=1;i<=s;i++)
        {
            x*=i;
        }
        Console.WriteLine(x);

        Console.ReadLine();
		}
	}
}
